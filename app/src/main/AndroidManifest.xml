<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pacman">

    <application
        android:allowBackup="true"
        android:icon="@drawable/pacmn_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pacmn_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="Menu.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Menu.SettingsActivity"></activity>
        <activity android:name="Menu.RecordsActivity"></activity>
        <activity android:name="Menu.RecordsUsualActivity"></activity>
        <activity android:name="Menu.GameModsActivity"></activity>
        <activity android:name="Menu.GameUsualActivity"></activity>
    </application>

</manifest>